{
    "name": "bitbag/shipping-export-plugin",
    "type": "sylius-plugin",
    "description": "Shipping data export environment for Sylius platform applications",
    "license": "MIT",
    "authors": [
        {
            "name": "Mikołaj Król",
            "email": "mikolaj.krol@bitbag.pl"
        },
        {
            "name": "Patryk Drapik",
            "email": "patryk.drapik@bitbag.pl"
        }
    ],
    "require": {
        "php": "^7.1",
        "sylius/sylius": "^1.0.4"
    },
    "require-dev": {
        "phpspec/phpspec": "^3.2",
        "phpunit/phpunit": "^5.6",
        "behat/behat": "^3.3",
        "behat/mink": "^1.7",
        "behat/mink-browserkit-driver": "^1.3",
        "behat/mink-extension": "^2.2",
        "behat/mink-selenium2-driver": "^1.3",
        "friends-of-behat/context-service-extension": "^1.0",
        "friends-of-behat/cross-container-extension": "^1.0",
        "friends-of-behat/service-container-extension": "^1.0",
        "friends-of-behat/symfony-extension": "^1.0",
        "friends-of-behat/variadic-extension": "^1.0",
        "lakion/mink-debug-extension": "^1.2.3",
        "se/selenium-server-standalone": "^2.52",
        "symplify/easy-coding-standard": "^2.4",
        "sylius-labs/coding-standard": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "BitBag\\SyliusShippingExportPlugin\\": "src/",
            "Tests\\BitBag\\SyliusShippingExportPlugin\\": "tests/"
        }
    },
    "prefer-stable": true,
    "autoload-dev": {
        "classmap": ["tests/Application/app/AppKernel.php"]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.3-dev"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
